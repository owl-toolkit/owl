{
    "dataset": {
        "full": [
            {
                "name": "base",
                "determinize": true
            },
            {
                "name": "regressions",
                "determinize": true
            },
            "size",
            "fgx",
            "check",
            "libe-spec",
            "random"
        ],
        "medium": [
            {
                "name": "base",
                "determinize": true
            },
            {
                "name": "regressions",
                "determinize": true
            },
            "size",
            "fgx",
            "random"
        ],
        "small": [
            {
                "name": "base",
                "determinize": true
            },
            {
                "name": "regressions",
                "determinize": true
            },
            "random"
        ]
    },
    "defaults": {
        "reference": {
            "name": "ltl2tgba",
            "exec": [
                "ltl2tgba",
                "-H",
                "-f",
                "%f"
            ]
        },
        "data": "full"
    },
    "tests": {
        "ltl2ldba": {
            "tools": {
                "default": "ltl2ldba#degeneralise",
                "non-det": "ltl2ldba#degeneralise,non-deterministic",
                "guess-F": "ltl2ldba#degeneralise,guess-f",
                "guess-F+non-det": "ltl2ldba#degeneralise,guess-f,non-deterministic"
            }
        },
        "ltl2ldba-small": {
            "tools": [
                "ltl2ldba#degeneralise,simple",
                "ltl2ldba#degeneralise,non-deterministic,simple",
                "ltl2ldba#degeneralise,guess-f,simple",
                "ltl2ldba#degeneralise,guess-f,non-deterministic,simple"
            ],
            "data": "small"
        },
        "ltl2ldgba": {
            "tools": {
                "default": "ltl2ldba#",
                "non-det": "ltl2ldba#non-deterministic",
                "guess-F": "ltl2ldba#guess-f",
                "guess-F+non-det": "ltl2ldba#guess-f,non-deterministic"
            }
        },
        "ltl2ldgba-small": {
            "tools": [
                "ltl2ldba#simple",
                "ltl2ldba#non-deterministic,simple",
                "ltl2ldba#guess-f,simple",
                "ltl2ldba#guess-f,non-deterministic,simple"
            ],
            "data": "small"
        },
        "ltl2dpa": {
            "tools": [
                "ltl2dpa",
                "ltl2dpa#complement"
            ]
        },
        "ltl2dpa-small": {
            "tools": [
                "ltl2dpa#guess-f",
                "ltl2dpa#guess-f,complement",
                "ltl2dpa#simple",
                "ltl2dpa#complement,simple",
                "ltl2dpa#guess-f,simple",
                "ltl2dpa#guess-f,complement,simple"
            ],
            "data": "small"
        },
        "ltl2da": "ltl2da",
        "nba2ldba": "spot-nba2ldba",
        "nba2dpa": {
            "tools": [
                "spot-nba2dpa"
            ],
            "data": "small"
        },
        "delag": {
            "tools": [
                "delag",
                "delag#fallback-spot"
            ],
            "data": "full"
        },
        "delag-strict": {
            "tools": "delag#strict",
            "data": "fgx"
        },
        "rabinizer": {
            "tools": [
                "rabinizer",
                "rabinizer-iar"
            ],
            "data": "medium"
        },
        "ltl2dpa-convert": {
            "tools": [
                "ltl2dpa-convert#max",
                "ltl2dpa-convert#min",
                "ltl2dpa-convert#even",
                "ltl2dpa-convert#odd",
                "ltl2dpa-convert#max,even",
                "ltl2dpa-convert#max,odd",
                "ltl2dpa-convert#min,even",
                "ltl2dpa-convert#min,odd"
            ],
            "data": "small"
        }
    }
}