{
    "dataset": {
        "full": [
            {
                "name": "base",
                "determinize": true
            },
            {
                "name": "regressions",
                "determinize": true
            },
            "size",
            "fgx",
            "check",
            "libe-spec",
            "random"
        ],
        "small": [
            {
                "name": "base",
                "determinize": true
            },
            {
                "name": "regressions",
                "determinize": true
            },
            "random"
        ]
    },
    "defaults": {
        "reference": {
            "name": "ltl2tgba",
            "exec": [
                "ltl2tgba",
                "-H",
                "-f",
                "%f"
            ]
        },
        "data": "full"
    },
    "tests": {
        "ltl2ldba": {
            "tools": [
                "ltl2ldba",
                "ltl2ldba#breakpoint",
                "ltl2ldba#breakpoint,deterministic",
                "ltl2ldba#deterministic"
            ]
        },
        "ltl2ldbga": {
            "tools": [
                "ltl2ldba#gen-buchi",
                "ltl2ldba#gen-buchi,breakpoint",
                "ltl2ldba#gen-buchi,deterministic,breakpoint",
                "ltl2ldba#gen-buchi,deterministic"
            ]
        },
        "ltl2dpa": {
            "tools": [
                "ltl2dpa",
                "ltl2dpa#complement"
            ]
        },
        "ltl2dpa-bp": {
            "tools": [
                "ltl2dpa#breakpoint",
                "ltl2dpa#breakpoint,complement"
            ],
            "data": "small"
        },
        "ltl2da": "ltl2da",
        "nba2ldba": "spot-nba2ldba",
        "nba2dpa": {
            "tools": [
                "spot-nba2dpa",
                "spot-nba2dpa#nosafety"
            ],
            "data": "small"
        },
        "delag": {
            "tools": [
                "delag",
                "delag#fallback-spot"
            ],
            "data": "full"
        },
        "delag-strict": {
            "tools": "delag#strict",
            "data": "fgx"
        },
        "rabinizer": {
            "tools": [
                "rabinizer",
                "rabinizer-iar"
            ],
            "data": "small"
        }
    }
}
