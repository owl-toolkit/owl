{
    "tools": {
        "fgx2dpa": {
            "type": "owl",
            "input": "ltl",
            "output": "hoa",
            "name": "fgx2dpa"
        },
        "ltl2ldba": {
            "type": "owl",
            "input": "ltl",
            "output": "hoa",
            "name": "ltl2ldba",
            "flags": {
                "degeneralise": "-d",
                "epsilon": "-e",
                "guess-f": "-f",
                "non-deterministic": "-n",
                "simple": "-s"
            },
            "pre": [
                "simplify-ltl"
            ]
        },
        "ltl2dpa": {
            "type": "owl",
            "input": "ltl",
            "output": "hoa",
            "name": "ltl2dpa",
            "flags": {
               "complement": "--complement",
               "guess-f": "--guess-f",
               "simple": "--simple"
            }
        },
        "ltl2dra": {
            "type": "owl",
            "input": "ltl",
            "output": "hoa",
            "name": "ltl2dra",
            "flags": {
               "degeneralise": "-d",
               "simple": "--simple"
            }
        },
        "ltl2da": {
            "type": "owl",
            "input": "ltl",
            "output": "hoa",
            "name": "ltl2da",
            "pre": [
                "simplify-ltl"
            ]
        },
        "delag": {
            "type": "owl",
            "input": "ltl",
            "output": "hoa",
            "name": "delag",
            "flags": {
                "strict": "--fallback=none",
                "fallback-spot": "--fallback=ltl2tgba -H --deterministic --generic"
            },
            "exclusive-flags": [
                [
                    "strict",
                    "fallback-spot"
                ]
            ],
            "pre": [
                "simplify-ltl"
            ]
        },
        "rabinizer-dgra": {
            "type": "owl",
            "input": "ltl",
            "output": "hoa",
            "name": "ltl2dgra",
            "flags": {
                "nosuspend": "--nosuspend",
                "noeager": "--noeager"
            },
            "pre": [
                "simplify-ltl"
            ],
            "post": [
                "minimize-aut"
            ]
        },
        "rabinizer-dra": {
            "type": "owl",
            "input": "ltl",
            "output": "hoa",
            "name": "ltl2dgra",
            "pre": [
                "simplify-ltl"
            ],
            "post": [
                "minimize-aut",
                "dgra2dra",
                "minimize-aut"
            ]
        },
        "rabinizer-dpa": {
            "type": "owl",
            "input": "ltl",
            "output": "hoa",
            "name": "dra2dpa",
            "pre": [
                "simplify-ltl",
                "ltl2dgra",
                "minimize-aut",
                "dgra2dra"
            ],
            "post": [
                "minimize-aut"
            ]
        },
        "spot-nba2ldba": {
            "type": "owl",
            "input": "ltl",
            "output": "hoa",
            "name": "nba2ldba",
            "pre": [
                "simplify-ltl",
                [
                    "ltl2aut-ext",
                    "-t",
                    "ltl2tgba -H"
                ]
            ]
        },
        "spot-nba2dpa": {
            "type": "owl",
            "input": "ltl",
            "output": "hoa",
            "name": "nba2dpa",
            "pre": [
                "simplify-ltl",
                [
                    "ltl2aut-ext",
                    "-t",
                    "ltl2tgba -H"
                ]
            ],
            "flags": {
                "nosafety": "--nosafety"
            }
        },
        "ltl2tgba": {
            "type": "spot",
            "executable": "ltl2tgba",
            "defaults": [
                "hoaf"
            ],
            "flags": {
                "hoaf": "--hoaf=1.1"
            }
        },
        "ltl2dpa-convert": {
            "type": "owl",
            "input": "ltl",
            "output": "hoa",
            "name": "convert-parity",
            "flags": {
                "max": "--max",
                "min": "--min",
                "even": "--even",
                "odd": "--odd"
            },
            "pre": [
                "simplify-ltl",
                "ltl2dpa"
            ],
            "post": [
                "complement-parity",
                "minimize-aut",
                "complement-parity"
            ]
        },
        "safra": {
            "type": "owl",
            "input": "ltl",
            "output": "hoa",
            "name": "safra",
            "pre": [
                "simplify-ltl",
                [
                    "ltl2aut-ext",
                    "-t",
                    "ltl2tgba -B"
                ]
            ],
            "post": [
                "minimize-aut"
            ]
        }
    },
    "aliases": {
    }
