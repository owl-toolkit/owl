{
    "tools": {
        "ltl2nba": {
            "type": "owl",
            "input": "ltl",
            "output": "hoa",
            "name": "ltl2nba",
            "flags": {
                "disable-portfolio": "--disable-portfolio"
            },
            "pre": [
                "simplify-ltl"
            ]
        },
        "ltl2ngba": {
            "type": "owl",
            "input": "ltl",
            "output": "hoa",
            "name": "ltl2ngba",
            "flags": {
                "disable-portfolio": "--disable-portfolio"
            },
            "pre": [
                "simplify-ltl"
            ]
        },
        "ltl2ldba": {
            "type": "owl",
            "input": "ltl",
            "output": "hoa",
            "name": "ltl2ldba",
            "flags": {
                "asymmetric": "--asymmetric",
                "symmetric": "--symmetric",
                "disable-portfolio": "--disable-portfolio"
            },
            "pre": [
                "simplify-ltl"
            ]
        },
        "ltl2ldgba": {
            "type": "owl",
            "input": "ltl",
            "output": "hoa",
            "name": "ltl2ldgba",
            "flags": {
                "asymmetric": "--asymmetric",
                "symmetric": "--symmetric",
                "disable-portfolio": "--disable-portfolio"
            },
            "pre": [
                "simplify-ltl"
            ]
        },
        "ltl2dpa": {
            "type": "owl",
            "input": "ltl",
            "output": "hoa",
            "name": "ltl2dpa",
            "flags": {
                "ldba-asymmetric":         "--ldba='--asymmetric'",
                "ldba-symmetric":          "--ldba='--symmetric'",
                "typeness-dra-asymmetric": "--typeness='--asymmetric'",
                "typeness-dra-symmetric":  "--typeness='--symmetric'",
                "iar-dra-asymmetric":      "TBD",
                "iar-dra-symmetric":       "TBD",
                "disable-portfolio":       "--disable-portfolio"
            },
            "pre": [
                "simplify-ltl"
            ]
        },
        "nbadet": {
            "type": "owl",
            "input": "ltl",
            "output": "hoa",
            "name": "nbadet",
            "flags": {
                "default": "--default-config",
                "mullerschupp": "-m0",
                "safra": "-m1",
                "maxmerge": "-m2",
                "powersets": "-t",
                "smartsucc": "-s",
                "directsim": "-lrefinement",
                "uselangext": "-e",
                "uselangint": "-j",
                "ascc": "-A",
                "ascccyc": "-b",
                "rscc": "-r",
                "optdet": "-d",
                "optmix": "-c"
            },
            "pre": [
                "simplify-ltl",
                [
                    "ltl2aut-ext",
                    "-t",
                    "'ltl2tgba -B -H'"
                ],
                "optimize-aut"
            ],
            "post": [
                "optimize-aut"
            ]
        },
        "ltl2dra": {
            "type": "owl",
            "input": "ltl",
            "output": "hoa",
            "name": "ltl2dra",
            "flags": {
                "asymmetric": "--asymmetric",
                "symmetric": "--symmetric",
                "disable-portfolio": "--disable-portfolio"
            },
            "pre": [
                "simplify-ltl"
            ],
            "post": [
                "optimize-aut"
            ]
        },
        "ltl2dgra": {
            "type": "owl",
            "input": "ltl",
            "output": "hoa",
            "name": "ltl2dgra",
            "flags": {
                "asymmetric": "--asymmetric",
                "symmetric": "--symmetric",
                "disable-portfolio": "--disable-portfolio"
            },
            "pre": [
                "simplify-ltl"
            ],
            "post": [
                "optimize-aut"
            ]
        },
        "ltl2dgra2dra": {
            "type": "owl",
            "input": "ltl",
            "output": "hoa",
            "name": "ltl2dgra",
            "flags": {
                "asymmetric": "--asymmetric",
                "symmetric": "--symmetric",
                "disable-portfolio": "--disable-portfolio"
            },
            "pre": [
                "simplify-ltl"
            ],
            "post": [
                "optimize-aut",
                "dgra2dra",
                "optimize-aut"
            ]
        },
        "ltl2da": {
            "type": "owl",
            "input": "ltl",
            "output": "hoa",
            "name": "ltl2da",
            "pre": [
                "simplify-ltl"
            ]
        },
        "ltl2na": {
            "type": "owl",
            "input": "ltl",
            "output": "hoa",
            "name": "ltl2na",
            "pre": [
                "simplify-ltl"
            ]
        },
        "delag": {
            "type": "owl",
            "input": "ltl",
            "output": "hoa",
            "name": "delag",
            "flags": {
                "strict": "--fallback=none",
                "fallback-spot": "--fallback='ltl2tgba -H --deterministic --generic'"
            },
            "exclusive-flags": [
                [
                    "strict",
                    "fallback-spot"
                ]
            ],
            "pre": [
                "simplify-ltl"
            ]
        },
        "delag-normalform": {
            "type": "owl",
            "input": "ltl",
            "output": "hoa",
            "name": "ltl2normalform",
            "flags": {
              "dual": "--dual",
              "local": "--local"
            },
            "pre": [
                "simplify-ltl"
            ],
            "post": [
                "simplify-ltl",
                "delag"
            ]
        },
        "rabinizer-dpa": {
            "type": "owl",
            "input": "ltl",
            "output": "hoa",
            "name": "ltl2dgra",
            "flags": {
                "asymmetric": "--asymmetric"
            },
            "pre": [
                "simplify-ltl"
            ],
            "post": [
                "optimize-aut",
                "dgra2dra",
                "optimize-aut",
                ["dra2dpa", "--min"],
                "optimize-aut"
            ]
        },
        "iar": {
            "type": "owl",
            "input": "ltl",
            "output": "hoa",
            "name": "dra2dpa",
            "flags": {
                "odd": "--odd",
                "min": "--min"
            },
            "pre": [
                "simplify-ltl",
                "ltl2dgra",
                "optimize-aut",
                "dgra2dra",
                "optimize-aut"
            ],
            "post": [
                "optimize-aut"
            ]
        },
        "spot-nba2ldba": {
            "type": "owl",
            "input": "ltl",
            "output": "hoa",
            "name": "nba2ldba",
            "pre": [
                "simplify-ltl",
                [
                    "ltl2aut-ext",
                    "-t",
                    "'ltl2tgba -H'"
                ]
            ]
        },
        "spot-nba2dpa": {
            "type": "owl",
            "input": "ltl",
            "output": "hoa",
            "name": "nba2dpa",
            "pre": [
                "simplify-ltl",
                [
                    "ltl2aut-ext",
                    "-t",
                    "'ltl2tgba -H'"
                ]
            ],
            "post": [
                "optimize-aut"
            ]
        },
        "ltl2tgba": {
            "type": "spot",
            "executable": "ltl2tgba",
            "defaults": [
                "hoaf"
            ],
            "flags": {
                "hoaf": "--hoaf=1.1",
                "deterministic": "-D",
                "any": "-a",
                "buchi": "-B",
                "complete": "-C",
                "parity": "-P"
            }
        },
        "ltl2dpa-convert": {
            "type": "owl",
            "input": "ltl",
            "output": "hoa",
            "name": "convert-parity",
            "flags": {
                "max": "--max",
                "min": "--min",
                "even": "--even",
                "odd": "--odd"
            },
            "pre": [
                "simplify-ltl",
                "ltl2dpa"
            ],
            "post": [
                "complement-parity",
                "optimize-aut",
                "complement-parity"
            ]
        },
        "buchi-degen": {
            "type": "owl",
            "input": "ltl",
            "output": "hoa",
            "name": "buchi-degeneralization",
            "pre": [
                "simplify-ltl",
                "ltl2ngba"
            ]
        },
        "nbasim": {
            "type": "owl",
            "input": "ltl",
            "output": "hoa",
            "name": "nbasim",
            "flags": {
                "refinement": "--direct-refinement",
                "direct": "--direct",
                "delayed": "--delayed"
            },
            "pre": [
                "simplify-ltl",
                [
                    "ltl2aut-ext",
                    "-t",
                    "'ltl2tgba -B -H'"
                ]
            ]
        }
    },
    "aliases": {
    }
}
