{
    "tools": {
        "ltl2ldba": {
            "executable": "build/bin/ltl2ldba",
            "defaults": [
                "buchi"
            ],
            "exclusive-flags": [
                [
                    "buchi",
                    "gen-buchi"
                ]
            ],
            "flags": {
                "gen-buchi": "--generalised-Buchi",
                "buchi": "--Buchi",
                "guess-f": "--guess-F",
                "nondet-init": "--nondeterministic-initial-component",
                "breakpoint-free": "--breakpoint-free"
            },
            "optimisations": {
                "no-eps": "REMOVE_EPSILON_TRANSITIONS"
            }
        },
        "ltl2dpa": {
            "executable": "build/bin/ltl2dpa",
            "optimisations": {
                "no-eps": "REMOVE_EPSILON_TRANSITIONS"
            },
            "flags": {
                "breakpoint-free": "--breakpoint-free"
            }
        },
        "ltl2da": {
            "executable": "build/bin/ltl2da"
        },
        "delag": {
            "executable": "build/bin/delag",
            "flags": {
                "strict": "--strict",
                "fallback": "--fallback='ltl2tgba -H --deterministic --generic'"
            }
        },
        "rabinizer": {
            "executable": "build/bin/rabinizer"
        },
        "ltl2tgba": {
            "executable": "ltl2tgba",
            "defaults": [
                "hoaf"
            ],
            "flags": {
                "hoaf": "--hoaf=1.1"
            }
        }
    },
    "aliases": {
    }
}